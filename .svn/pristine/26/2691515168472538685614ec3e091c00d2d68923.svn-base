<!-- 分享sdk -->
<script type="text/javascript" src="https://res.wx.qq.com/open/js/jweixin-1.6.0.js"></script>
<script type="text/javascript" src="https://g.alicdn.com/dingding/open-develop/1.9.0/dingtalk.js"></script>
<script type="text/javascript" src="https://yzf.qq.com/xv/web/static/chat_sdk/yzf_chat.min.js"></script>
<script type="text/javascript">
  //app搜索页面、协议页面、分享页面、下载页面、编辑器不展示客服按钮 
  var url = window.location.href;
  var whiteList = new Array("/appsearch", "/protocol", "/share", "/download", "/mEditor", "/liveBox");
  var showkf = true;
  for(var i = 0; i < whiteList.length; i++) {
    if(url.indexOf(whiteList[i]) > 0) {
      showkf = false;
      break;
    }
  }
  if (showkf) {
    //参数说明
    //sign：公司渠道唯一标识，复制即可，无需改动
    //uid：用户唯一标识，如果没有则不填写，默认为空
    //data：用于传递用户信息，最多支持5个，参数名分别为c1,c2,c3,c4,c5；默认为空
    //selector：css选择器(document.querySelector, 如#btnid .chat-btn等)，用于替换默认的常驻客服入口
    //callback(type, data): 回调函数,type表示事件类型， data表示事件相关数据
    //type支持的类型：newmsg有新消息，error云智服页面发生错误， close聊天窗口关闭
    var user = window.localStorage.getItem("userInfo") !== '""' ? JSON.parse(window.localStorage.getItem("userInfo")) : undefined;
    var userName = user ? user.nickName : "游客"
    var userMobile = user ? user.mobile : ""
    var userAccount = user ? user.accountId : ""
    setInterval(() => {
      window.yzf && window.yzf.init({
      sign: '37ef9b97d77303902344cbeb19b5e560fd1c6c5043643e4120a992abefbdb82a87a9c50e53317cb6fc3ff83af3d8e09eb5819492',
      uid: userAccount,
      data: {
        c1: userName,
        c2: userMobile,
        c3: userAccount,
        c4: '',
        c5: ''
      },
      selector: '#onlineService',
      callback: function (type, data) { }
    });
    }, 1000);

    
  }
</script>

</html>